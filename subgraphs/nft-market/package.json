{
  "name": "nft-market",
  "description": "MoleSwap NFT Market Subgraph",
  "version": "1.0.0",
  "repository": "git@github.com:MoleTech-io/pancake-subgraph.git",
  "author": "MoleSwap",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build --ipfs https://api.staging.thegraph.com/ipfs/",
    "create-local": "graph create moleswap --node http://127.0.0.1:8020",
    "deploy-local": "graph deploy moleswap --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020/",
    "deploy": "graph deploy --product hosted-service --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ moletech-io/moleswap subgraph.yaml"
  },
  "dependencies": {
    "ganache-cli": "^6.12.2"
  }
}
